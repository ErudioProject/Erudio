  "ignorePatterns": [
    "**/*",
    "**/node_modules"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ]
