    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ]
      "rules": {}
    },
    {
      "files": [
        "*.spec.ts",
        "*.spec.tsx",
        "*.spec.js",
        "*.spec.jsx"
      ]
      "rules": {}
    }
  ]
}