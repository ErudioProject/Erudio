{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "frontend-data-access-i18n",
  "sourceRoot": "libs/frontend/data-access/i18n/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/frontend/data-access/i18n",
        "main": "libs/frontend/data-access/i18n/src/index.ts",
        "tsConfig": "libs/frontend/data-access/i18n/tsconfig.lib.json",
        "assets": ["libs/frontend/data-access/i18n/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/frontend/data-access/i18n/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/frontend/data-access/i18n/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
